FUNCTION TEST_GET_VIIRS_LAT_LON

; LIKE A UNIT TEST FOR GET_VIIRS_LAT_LON

; READ IN THE TEST FILE AND PRINT THE LAT AND LON TO THE SCREEN
CD, CURRENT=c
VIIRS_TEST_FILE = c + '/Source/viirs/test/' + 'NPP_VMAE_L1.A2013176.0030.P1_03001.2013176093155.hdf'
TEST = GET_VIIRS_LAT_LON(VIIRS_TEST_FILE)

IF TEST.LAT[0,0] eq -55.41022 THEN BEGIN
  PRINT, 'TEST PASSED'
ENDIF ELSE BEGIN 
  PRINT, TEST.LAT[0]
  RETURN, 0  
ENDELSE
RETURN, 1
END